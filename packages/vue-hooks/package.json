{
  "name": "@medplum/vue-hooks",
  "version": "0.0.1",
  "description": "Medplum Vue Hooks for Cordina",
  "keywords": [
    "medplum",
    "plugin",
    "vue"
  ],
  "homepage": "https://github.com/vueschool/custom-vue-js-3-plugins-course",
  "bugs": {
    "url": "https://github.com/vueschool/custom-vue-js-3-plugins-course/issues"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    "import": {
      "types": "./dist/esm/index.d.ts",
      "default": "./dist/esm/index.mjs"
    },
    "require": {
      "types": "./dist/cjs/index.d.ts",
      "default": "./dist/cjs/index.cjs"
    }
  },
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "types": "dist/cjs/index.d.ts",
  "files": [
    "dist/cjs",
    "dist/esm"
  ],
  "scripts": {
    "api-extractor": "api-extractor run --local && cp dist/types.d.ts dist/cjs/index.d.ts && cp dist/types.d.ts dist/esm/index.d.ts",
    "build": "npm run clean && tsc && node esbuild.mjs && npm run api-extractor",
    "clean": "rimraf dist",
    "test": "jest"
  },
  "devDependencies": {
    "@medplum/core": "3.2.0",
    "@medplum/definitions": "3.2.0",
    "@medplum/fhirtypes": "3.2.0",
    "@medplum/mock": "3.2.0",
    "@types/jest": "29.5.12",
    "@types/node": "20.14.9",
    "jest": "29.7.0",
    "jest-each": "29.7.0",
    "jest-websocket-mock": "2.5.0",
    "rimraf": "5.0.7",
    "typescript": "5.5.2",
    "vue": "^3.4.31"
  },
  "peerDependencies": {
    "@medplum/core": "3.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
